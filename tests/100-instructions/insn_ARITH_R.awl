	// Precalculate constants
	// MD 0 = PI
	// MD 4 = -PI
	// MD 8 = PI/2
	// MD 12 = -PI/2
	// MD 16 = PI/4
	// MD 20 = -PI/4
	L		__CNST_PI
	T		MD 0
	NEGR
	T		MD 4

	L		__CNST_PI
	L		2.0
	/R
	T		MD 8
	NEGR
	T		MD 12

	L		__CNST_PI
	L		4.0
	/R
	T		MD 16
	NEGR
	T		MD 20


	// Run tests


