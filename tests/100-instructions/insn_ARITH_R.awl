	L		1.0
	L		1.0
	+R
	__ASSERT==	__ACCU 1,	2.0
	__ASSERT==	__ACCU 1,	DW#16#40000000
	__ASSERT==	__STW A1,	1
	__ASSERT==	__STW A0,	0
	__ASSERT==	__STW OV,	0
	__ASSERT==	__STW OS,	0


