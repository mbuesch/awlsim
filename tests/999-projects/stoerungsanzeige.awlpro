FUNCTION_BLOCK "stoeranzeige_fb"
TITLE =
VERSION : 0.1


VAR_INPUT
  stoerung : BOOL ;	
  quittieren : BOOL ;	
  blinktakt : BOOL ;	
END_VAR
VAR_OUTPUT
  anzeige : BOOL ;	
END_VAR
VAR
  meldespeicher : BOOL ;	
  flankenmerker : BOOL ;	
END_VAR
BEGIN
NETWORK
TITLE =

      U(    ; 
      U     #quittieren; 
      R     #meldespeicher; 
      U     #stoerung; 
      FP    #flankenmerker; 
      S     #meldespeicher; 
      U     #meldespeicher; 
